type Query {
    product: [Product]
}

type Product {
    id: ID!
    name: String!
    category: String!
    SKU: String
    image: [ProductImage]
    description: String
    status: String
    slug: String!
    supplier: String
}

type ProductImage {
    id: ID!
    name: String
    imagePath: String
    alt: String
    isDefault: Boolean
}
